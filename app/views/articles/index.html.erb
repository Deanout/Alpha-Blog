<h1>Home</h1>
<h2>Listing All Articles</h2>
<p><%= link_to "Create New Article", new_article_path %></p>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
  <% @articles.each do |article| %>
  <tr>
    <td><%= article.title %></td>
    <td><%= article.description %></td>
    <td><%= link_to "Edit", edit_article_path(article) %></td>
    <td><%= link_to "View", article_path(article) %></td>
  </tr>
  <% end %>
</table>